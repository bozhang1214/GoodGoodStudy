  }

  /**
   * è·å–å½“å‰ç”¨æˆ·ID
   */
  async getCurrentUserId(): Promise<number> {
    await this.ensureInit();
    const value = await this.prefsStore!.get(AppConstants.KEY_USER_ID, -1);
